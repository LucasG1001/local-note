{
  "name": "desktop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "npx wait-on http://localhost:5173 && npx electron .",
    "build": "tsc -b && vite build",
